{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "strict": true,
    "moduleResolution": "node12",
    "esModuleInterop": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "skipLibCheck": true,
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "forceConsistentCasingInFileNames": true,
    "preserveWatchOutput": true
  }
}