{
  "name": "@chookscord/cli",
  "version": "2.0.0",
  "description": "CLI package for the Chooksie Framework",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "chooks": "./dist/bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "build": "concurrently -r 'yarn:build:*'",
    "build:swc": "swc src -d dist --config-file ../../.swcrc",
    "build:tsc": "tsc",
    "dev": "concurrently -r 'yarn:build:* -w'"
  },
  "dependencies": {
    "@swc/core": "^1.2.130",
    "@swc/helpers": "^0.3.2",
    "chokidar": "^3.5.2",
    "create-chooks-bot": "workspace:^2.0.0",
    "joi": "^17.5.0",
    "picocolors": "^1.0.0",
    "pino-abstract-transport": "^0.5.0",
    "pino-pretty": "^7.5.1",
    "sonic-boom": "^2.6.0"
  },
  "devDependencies": {
    "chooksie": "workspace:^2.0.0",
    "discord.js": "^13.6.0",
    "dotenv": "^15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chookscord/framework.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  }
}
