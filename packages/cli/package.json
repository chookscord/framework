{
  "name": "@chookscord/cli",
  "version": "1.0.1-rc.1",
  "license": "MIT",
  "preferGlobal": true,
  "exports": {
    "./package.json": "./package.json",
    "./lib": "./dist/lib/index.js",
    "./build": "./dist/scripts/build.js",
    "./register": "./dist/scripts/register.js",
    "./dev": "./dist/scripts/dev.js",
    "./scaffold": "./dist/scripts/scaffold.js",
    "./templates": "./templates"
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./dist/lib/index.d.ts"
      ]
    }
  },
  "bin": {
    "chooks": "./bin/chooks.js"
  },
  "files": [
    "templates",
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "npx tsc",
    "build:dev": "npx tsc-watch --onCompilationComplete 'cp src/scripts/entrypoint.ts dist/scripts'",
    "build:bin": "npx tsc -p tsconfig.bin.json",
    "clean-build": "rm -rf dist bin && concurrently 'yarn build' 'yarn build:bin'",
    "dev": "concurrently 'yarn build:dev' 'yarn build:bin -w'"
  },
  "dependencies": {
    "@chookscord/logger": "^1.0.0",
    "@swc/core": "^1.2.112",
    "@swc/helpers": "^0.2.14",
    "chokidar": "^3.5.2",
    "dotenv": "^10.0.0",
    "nanospinner": "^0.5.0",
    "package-name-regex": "^2.0.4",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/package-name-regex": "^2.0.0",
    "@types/prompts": "^2.0.14",
    "chooksie": "^1.0.0-rc.1",
    "discord.js": "^13.3.1",
    "node-fetch": "^2.6.5"
  }
}
