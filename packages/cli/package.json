{
  "name": "@chookscord/cli",
  "version": "3.0.0-dev.0",
  "description": "CLI package for the Chooksie Framework",
  "license": "MIT",
  "type": "module",
  "bin": {
    "chooks": "./dist/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "concurrently -r 'yarn:build:*'",
    "build:swc": "npx swc src -d dist --config-file .swcrc",
    "build:tsc": "tsc",
    "dev": "concurrently -r 'yarn:build:* -w'"
  },
  "dependencies": {
    "@swc/core": "^1.2.147",
    "@swc/helpers": "^0.3.6",
    "chokidar": "^3.5.3",
    "create-chooks-bot": "workspace:*",
    "joi": "^17.6.0",
    "picocolors": "^1.0.0",
    "pino-abstract-transport": "^1.0.0",
    "sonic-boom": "^3.0.0",
    "undici": "^5.6.0"
  },
  "devDependencies": {
    "chooksie": "workspace:*",
    "discord.js": "^13.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chookscord/framework.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  }
}
