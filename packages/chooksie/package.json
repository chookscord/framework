{
  "name": "chooksie",
  "version": "2.0.0",
  "description": "Fast Discord.JS Framework",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build:swc": "swc src -d dist --config-file ../../.swcrc",
    "build:tsc": "tsc",
    "dev": "concurrently -r 'pnpm:build:* -- -w'"
  },
  "dependencies": {
    "@swc/helpers": "^0.3.2",
    "dotenv": "^15.0.0",
    "pino": "^7.6.5"
  },
  "devDependencies": {
    "discord.js": "^13.6.0",
    "eslint-config-lint": "workspace:*",
    "jest-config": "workspace:*",
    "tsconfig": "workspace:*",
    "undici": "^4.13.0"
  },
  "peerDependencies": {
    "discord.js": "^13.6.0",
    "undici": "^4.13.0"
  },
  "main": "./dist/types.js",
  "exports": {
    ".": "./dist/types.js",
    "./fetch": "./dist/fetch.js",
    "./internals": "./dist/internals/index.js"
  },
  "typesVersions": {
    "*": {
      "fetch": ["./dist/fetch.d.ts"]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chookscord/framework.git",
    "directory": "packages/chooksie"
  },
  "keywords": [
    "discord",
    "discord.js",
    "typescript",
    "framework",
    "bot"
  ]
}