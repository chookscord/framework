{
  "name": "chooksie",
  "version": "3.0.0-dev.0",
  "description": "Fast Discord.JS Framework",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*.{js,d.ts}"
  ],
  "scripts": {
    "test": "jest",
    "build": "concurrently -r 'yarn:build:*'",
    "build:swc": "swc src -d dist --config-file ../../.swcrc",
    "build:tsc": "tsc",
    "dev": "concurrently -r 'yarn:build:* -w'"
  },
  "dependencies": {
    "@swc/helpers": "^0.3.6",
    "dotenv": "^16.0.1",
    "pino": "^8.1.0",
    "uid": "^2.0.0"
  },
  "devDependencies": {
    "discord.js": "^13.8.1",
    "undici": "^5.6.0"
  },
  "peerDependencies": {
    "discord.js": "^13.7.0",
    "undici": "^5.2.0"
  },
  "peerDependenciesMeta": {
    "undici": {
      "optional": true
    }
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./dotenv": "./dist/dotenv.js",
    "./fetch": "./dist/fetch.js",
    "./internals": "./dist/internals/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "fetch": [
        "./dist/fetch.d.ts"
      ],
      "dotenv": [
        "./dist/dotenv.d.ts"
      ],
      "package.json": [
        "./package.json"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chookscord/framework.git",
    "directory": "packages/chooksie"
  },
  "keywords": [
    "discord",
    "discord.js",
    "typescript",
    "framework",
    "bot"
  ]
}
