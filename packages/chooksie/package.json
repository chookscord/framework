{
  "name": "chooksie",
  "version": "1.0.2",
  "description": "Next Generation Discord.JS Framework",
  "license": "MIT",
  "type": "commonjs",
  "types": "dist/typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typings/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./lib": {
      "types": "./dist/typings/lib/index.d.ts",
      "require": "./dist/lib/index.js",
      "import": "./dist/lib/index.mjs"
    },
    "./types": {
      "types": "./dist/typings/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./dist/typings/lib/index.d.ts"
      ],
      "types": [
        "./dist/typings/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc",
    "build:wrap": "concurrently 'gen-esm-wrapper ./dist ./dist/index.mjs' 'gen-esm-wrapper ./dist/lib ./dist/lib/index.mjs'",
    "clean": "rm -rf dist",
    "clean-build": "yarn clean && yarn build",
    "dev": "tsc-watch --onCompilationComplete 'yarn build:wrap'"
  },
  "dependencies": {
    "@chookscord/fetch": "^1.0.0",
    "@chookscord/logger": "^1.0.0",
    "@swc/helpers": "^0.3.2",
    "dotenv": "^10.0.0",
    "picocolors": "^1.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "discord.js": "^13.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chookscord/framework.git"
  },
  "keywords": [
    "discord",
    "discordjs",
    "discord.js",
    "typescript",
    "framework",
    "client",
    "bot",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  }
}
