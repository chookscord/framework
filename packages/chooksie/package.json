{
  "name": "chooksie",
  "version": "2.0.0-rc.1",
  "description": "Fast Discord.JS Framework",
  "license": "MIT",
  "files": [
    "dist/**/*.{js,d.ts}"
  ],
  "scripts": {
    "test": "jest",
    "build": "concurrently -r 'yarn:build:*'",
    "build:swc": "swc src -d dist --config-file ../../.swcrc",
    "build:tsc": "tsc",
    "dev": "concurrently -r 'yarn:build:* -w'"
  },
  "dependencies": {
    "@swc/helpers": "^0.3.2",
    "dotenv": "^16.0.0",
    "pino": "^7.8.0"
  },
  "devDependencies": {
    "discord.js": "^13.6.0",
    "undici": "^4.14.1"
  },
  "peerDependencies": {
    "discord.js": "^13.6.0",
    "undici": "^4.13.0"
  },
  "main": "./dist/types.js",
  "exports": {
    ".": "./dist/types.js",
    "./fetch": "./dist/fetch.js",
    "./internals": "./dist/internals/index.js"
  },
  "typesVersions": {
    "*": {
      "fetch": [
        "./dist/fetch.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chookscord/framework.git",
    "directory": "packages/chooksie"
  },
  "keywords": [
    "discord",
    "discord.js",
    "typescript",
    "framework",
    "bot"
  ]
}
