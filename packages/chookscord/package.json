{
  "name": "chookscord",
  "version": "0.14.2",
  "description": "The next generation Discord.JS Framework for advanced and rapid bot development.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./discordjs": "./dist/djs.js",
    "./types": "./dist/types/index.js"
  },
  "typesVersions": {
    "*": {
      "discordjs": [
        "./dist/djs.d.ts"
      ],
      "types": [
        "./dist/types/public.d.ts"
      ]
    }
  },
  "bin": {
    "chooks": "dist/bin/chooks.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsc --outDir dist",
    "clean-build": "rm -rf dist && yarn build",
    "dev": "yarn build --watch"
  },
  "dependencies": {
    "@chookscord/lib": "^0.14.1",
    "@chookscord/types": "^0.3.0",
    "@swc/core": "^1.2.81",
    "@swc/helpers": "^0.2.13",
    "chokidar": "^3.5.2",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "tslib": "^2.3.1"
  },
  "license": "MIT",
  "keywords": [
    "discord",
    "discordjs",
    "discord.js",
    "typescript",
    "framework",
    "client",
    "bot",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chookscord/framework.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
